		<property name="apps">
			<set>
				<ref bean="kenyaemr.app.reports" />
			</set>
		</property>
		<property name="template" value="gbv:gbv_template.xls" />
		<property name="repeatingSection" value="sheet:1,row:2,dataset:gbvEnrollmentRegister" />
	</bean>

